#include <fstream>
#include <random>
#include <iostream>

int main(int argc, char *argv[]) {
    std::ofstream out("feed.bin", std::ios::binary);
    if (!out) {
        std::cerr << "Failed to open feed.bin for writing.\n";
        return 1;
    }

    std::mt19937 rng(12345);

    for (int i = 0; i < 10000; ++i) {
        uint64_t id = rng();
        uint32_t qty = rng() % 1000;
        double px = static_cast<double>(rng() % 100000) / 100.0;
        out.write(reinterpret_cast<const char *>(&id), sizeof(id));
        out.write(reinterpret_cast<const char *>(&qty), sizeof(qty));
        out.write(reinterpret_cast<const char *>(&px), sizeof(px));
    }

    std::cout << "Generated 10K synthetic orders -> feed.bin\n";
    return 0;
}


